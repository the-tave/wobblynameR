data.frame(a = c("duck", "duck", "goose"))
getperc <- function(data, var, val){
# data must be a dataframe, var must be a variable in this data fram. GOTCHA: var must be written in "" , e.g. "var".
# Val must be a value in var
n_data <- dim(data)[1]
round(((table(data[var])/n_data)[[val]])*100, 2) # %>% round(2)
}
getperc(data.frame(a = c("duck", "duck", "goose")), "a", "duck")
getperc(data.frame(a = c("duck", "duck", "goose")), "a", "goose")
getperc(data.frame(a = c("duck", "duck", "goose")), 1, "goose")
getperc(data.frame(a = c("duck", "duck", "goose")), 2, "goose")
getperc(data.frame(a = c("duck", "duck", "goose")), 0, "goose")
getperc(data.frame(a = c("duck", "duck", "goose")), "a", 1)
getperc(data.frame(a = c("duck", "duck", "goose")), "a", 2)
use_r(getperc)
